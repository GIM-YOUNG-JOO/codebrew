<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.mycompany.codebrew.dao.mybatis.ProductDao">
	<insert id="insert" parameterType="Product">
		<selectKey keyProperty="prid" order="BEFORE" resultType="int">
			select prid_seq.nextval from dual
		</selectKey>
		insert into product(prid, prname, pcid, prprice)
		values(${prid}, 'Test', 1, ${prprice})
	</insert>
</mapper>